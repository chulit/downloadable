{
    "name": "diskominfotik/downloadable",
    "description": "Diskominfotik File Converter and Download Helper for Laravel",
    "keywords": [
        "diskominfotik",
        "downloadable"
    ],
    "homepage": "https://git.jakarta.go.id/diskominfotik/downloadable",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Abu Abbas",
            "email": "notificationfor.gitlabs@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/support": "^9.2",
        "rap2hpoutre/fast-excel": "^4.1"
    },
    "require-dev": {
        "orchestra/testbench": "^7.6",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Diskominfotik\\Downloadable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Diskominfotik\\Downloadable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Diskominfotik\\Downloadable\\DownloadableServiceProvider"
            ],
            "aliases": {
                "Downloadable": "Diskominfotik\\Downloadable\\Facades\\Downloadable"
            }
        }
    }
}
